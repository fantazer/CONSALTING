.main-section-filter
	position: absolute
	top: 0
	left: 0
	background: rgba(10, 10, 10, 0.6)
	width: 100%
	height: 100%

.header-logo
	up()
	font-size: 20px
	color white
	text-align: left
	.icon
		fill $red
		size 120px
.header-phone
	up()
	font-size: 20px
	a
		color white

.main-section
	width: 100%
	height: 100vh
	background: rgba(0, 0, 0, 0.61)
	position: relative
	flex(mid)
	background-attachment fixed
	background-repeat: no-repeat
	bg-full()

.main-section-cont
	position: relative
	z-index 10
	f()
	fw()
	fl()
	margin-top: 5%
	padding: 0 20px

.main-section-title
	+below(640px)
		font-size: 24px
	up()
	color white
	font-size: 4vw
	font-weight:bold
	//text-align: center

.main-section-title-sub
	//text-align: center
	color white
	margin 20px 0 40px 0
.header
	//background-color rgba(0, 0, 0, 0.25)
	position: fixed
	top 0
	left: 0
	width: 100%
	z-index 50

.header-logo
	+below(1024px)
		width: 100px
	height: 40px
	width: 15%
	f()
	align-items: center
	img
		width: 100%

.header-wrap
	flex(between)

.header-nav__el-link-wrap
	+below(1024px)
		border-bottom:1px solid rgba(236, 236, 236, 0.09)
	flex(between)

.menu-toggle
	+above(1024px)
		display: none
	.icon-el
		cp()
		size 20px
		fill black
	.icon-el--close
		display: none
.menu-toggle--close
	.icon-el--close
		display: block
	.icon-el--open
		display: none
.header-nav__el
	+below(1200px)
		font-size: 12px
	+below(1024px)
		width: 100%
		font-size: 14px
		pl20px
	font-size: 14px
	position: relative
	//text-transform: uppercase
	font-weight:bold
	.icon-el
		size 10px
		fill white
		margin-left: 5px
		tr(all)
	&:hover
		&>.header-nav__sub
			opacity 1
			visibility visible
		.header-nav__sub
			transform translateY(0px)
		.icon-el
			+above(1024px)
				transform rotate(-180deg)

.header-nav__el-link
	+below(1024px)
		width: 100%
		flex(between)
		padding: 20px 10px
	padding: 10px 0
	display: inline-block
	color white
	tr(all)
	&:hover
		color darken(white,15)
.header-layer
	padding: 10px 0
.header-nav__el-icon
	.icon-el
		fill black
	+below(1024px)
		cp()
		flex(mid)
		background: darken(grey,20)
		size: 60px
		display: none
		.icon-el
			size 15px

.header-nav
	+below(1024px)
		position: fixed
		top 61px
		right: 0
		width: 300px
		height: calc(100vh - 61px)
		background: white
		justify-content:initial
		transform translateX(100%)
		tr(all)
		z-index 1100
		fl()
	+above(1024px)
		display: flex !important
	width: 60%
	justify-content:space-around

.header-nav--open
	+below(640px)
		width: 100%
	transform translateX(0%)
	overflow-y: auto
	.header-nav__el-link
		color black!important
.header-nav__sub
	+below(1024px)
		transform translateY(0px)
		position: initial
		width: 100%
		top: initital
		left: initial
		margin-left: 0px
		border-radius 0px
		opacity 1!important
		visibility visible!important
		transition initial
		border 0
		.window-coner-top
			display: none
	tr(all)
	transform translateY(20px)
	position: absolute
	width: 180px
	top: calc(100% + 12px)
	left: 50%
	margin-left: -90px
	border-radius 4px
	border 1px solid #dddddd
	background-color #ffffff
	opacity 0
	visibility hidden
	z-index 100

.header-nav__sub-wrap
	+below(1024px)
		background: #f2f2f2
	position: relative
	background: white

.header-nav__sub--hidden
	opacity 0
	visibility hidden
	height: 0

.header-nav__sub-el
	+below(1024px)
		padding: 10px 20px
		font-weight:200
	tr(all)
	padding: 20px
	display: block
	border-bottom:1px solid #f7f6f6
	z-index 5
	position: relative
	&:hover
		background: #f0f0f0
	&:last-child
		border-bottom: 0

.header-nav__el-link--mark
	+below(1024px)
		border-bottom 1px solid $border-color
	border-bottom 2px solid $red

.section-advant--about
	padding-top: 0!important

.header-main-page
	.menu-toggle
		.icon-el
			fill white
	.header-logo
		.icon
			fill white
	.header-nav__el-icon
		.icon-el
			fill white
